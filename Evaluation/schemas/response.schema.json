{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ContactCard",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "name",
    "email"
  ],
  "additionalProperties": false
}